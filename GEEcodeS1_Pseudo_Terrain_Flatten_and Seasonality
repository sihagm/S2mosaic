var collection = ee.ImageCollection('COPERNICUS/S1_GRD')
    .filterDate('2019-01-01', '2019-03-15')
    .filterBounds(geometry2)

var composite = collection.median().clip(geometry2).select('VH')

// Display a VH mosaic.

var vizParams = {
  min: -26,
  max: -2,
  gamma: 1.1
  
};

Map.addLayer(composite, vizParams, 'VH')

// Load the Sentinel-1 ImageCollection.
var sentinel1 = ee.ImageCollection('COPERNICUS/S1_GRD')
                .filterBounds(geometry3)
                .filterDate('2019-04-01', '2019-04-30');


// Combine ascending and descending passes to create a layer which is somehow similar to a terrain flattening
// (pseudo-)Terrain flattening:
var vh = sentinel1
  .filter(ee.Filter.eq('instrumentMode', 'IW'))
  .select('VH');

// Filter to get images from different look angles.
var vhAscending = vh.filter(ee.Filter.eq('orbitProperties_pass', 'ASCENDING'));
var vhDescending = vh.filter(ee.Filter.eq('orbitProperties_pass', 'DESCENDING'));

var asc = vhAscending.mean().clip(geometry3);
var des = vhDescending.mean().clip(geometry3);
Map.addLayer(asc, vizParams, 'asc');
Map.addLayer(des, vizParams, 'des');

var vizParams3 = {
  min: -25,
  max: -3,
  gamma: 1
  
};

var combined = (asc.add(des)).divide(2).clip(geometry2);

// Use these visualization parameters, customized by location.
var visParams3 = {min: -27, max: -6, palette: elevationPalette};


Map.addLayer(combined, vizParams3, 'combined');

/////////////////////////////////////////////////////
// seasonality with pseudo terrain flattening

var sentinel1_2 = ee.ImageCollection('COPERNICUS/S1_GRD')
    .filterBounds(geometry2)
var vh2 = sentinel1_2
  .filter(ee.Filter.eq('instrumentMode', 'IW'))
  
var vhAscending2 = vh2.filter(ee.Filter.eq('orbitProperties_pass', 'ASCENDING'));
var vhDescending2 = vh2.filter(ee.Filter.eq('orbitProperties_pass', 'DESCENDING'));

var springasc = ee.Image(vhAscending2.filterDate('2019-04-01', '2019-05-31').median().select('VH').clip(geometry2));
var summerasc = ee.Image(vhAscending2.filterDate('2019-06-01', '2019-08-31').median().select('VH').clip(geometry2));
var winterasc = ee.Image(vhAscending2.filterDate('2019-01-01', '2019-02-28').median().select('VH').clip(geometry2));

var springdes = ee.Image(vhDescending2.filterDate('2019-04-01', '2019-05-31').median().select('VH').clip(geometry2));
var summerdes = ee.Image(vhDescending2.filterDate('2019-06-01', '2019-08-31').median().select('VH').clip(geometry2));
var winterdes = ee.Image(vhDescending2.filterDate('2019-01-01', '2019-02-28').median().select('VH').clip(geometry2));

var spring = (springasc.add(springdes)).divide(2);
var summer = (summerasc.add(summerdes)).divide(2);
var winter = (winterasc.add(winterdes)).divide(2);

// Create a composite from means at look angles and seasonal backscatter.
var composite2 = ee.Image.cat([
spring, summer, winter
]);

/*var vizParams2 = {
  min: -25,
  max: 0,
  gamma: [1.1, 1.2, 0.9]
}; */

var vizParams2 = {
  min: -25,
  max: 0,
  gamma: [1, 1, 1]
};

Map.addLayer(composite2, vizParams2, 'seasonality terrain flattened composite');

// Export the result into GoogleDrive

var result = composite2.visualize(vizParams2);

 Export.image.toDrive({
  image: result,
  description: 'S1_mosaic_seasonality_2019',
  region: geometry4,
  maxPixels: 5000000000,
  //crs: 'EPSG:2056' // EPSG:2056 equals CH1903+ LV95, however GEE cannot reproject it into this, it has to be done later in ArcGIS
});  

Legend of the seasonality map:
// Red: Spring dry (Summer and winter more moist than spring)
// Green: Summer dry
// Blue: Winter dry
// Cyan: Dry in summer and winter (Spring moist)
// Magenta: dry in winter ad spring (Summer moist)
// White: moisture equal through year
// Brown: little bit more dry in summer
// Black: Waterbodies
